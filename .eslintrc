{
  "env": { "es6": true },
  "extends": ["react-app", "prettier"],
  "rules": {
    "jsx-quotes": [1, "prefer-double"],
    "no-var": "error",
    // HtmlAcademy
    "arrow-parens": ["error", "always"],
    "prefer-rest-params": "error",
    "prefer-spread": "error",
    "quotes": ["error", "backtick"]
  },
  "plugins": ["prettier"]
}
